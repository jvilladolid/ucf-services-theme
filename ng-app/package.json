{
  "scripts": {
    "//Descriptions": {
      "test": "Default command to run javascript tests.",
      "install:global": "Choose to install reusable developer dependecies globally."
    },
    "test": "echo \"Error: no test specified\" && exit 1",
    "install:global": "npm install -g typescript typings tslint jspm",
    "tslint": "tslint ./**/*.ts -t verbose -c ./tslint.json -e ./**/jspm_packages/** -e ./**/node_modules/** -e ./**/typings/**",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "build": "node ../scripts/build_templates.js",
    "build:w": "node ../scripts/build_templates.js -w",
    "bundle:app": "jspm bundle main - @angular/* - @angular/http - ng2-bootstrap/ng2-bootstrap - rxjs - ie-shim - zone.js bundle-app.js --inject --minify",
    "bundle:dep": "jspm bundle main - [main.js] - [app.module.js] - [./app-student-services/**/*] - [./calendar/**/*] - [campaign/**/*] - [pipes/**/*] bundle-dependencies.js --inject --minify",
    "bundle": "npm run bundle:app && npm run bundle:dep",
    "depcache": "jspm depcache main",
    "postinstall": "typings install"
  },
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/upgrade": "^2.4.1",
    "core-js": "^2.4.1",
    "ie-shim": "^0.1.0",
    "moment": "2.15.1",
    "ng2-bootstrap": "1.1.16",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.1",
    "zone.js": "^0.7.4"
  },
  "jspm": {
    "registry": "npm",
    "dependencies": {
      "@angular/common": "^2.4.1",
      "@angular/compiler": "^2.4.1",
      "@angular/core": "^2.4.1",
      "@angular/forms": "^2.4.1",
      "@angular/http": "^2.4.1",
      "@angular/platform-browser": "^2.4.1",
      "@angular/platform-browser-dynamic": "^2.4.1",
      "@angular/upgrade": "^2.4.1",
      "core-js": "^2.4.1",
      "ie-shim": "^0.1.0",
      "moment": "2.15.1",
      "ng2-bootstrap": "1.1.16",
      "reflect-metadata": "0.1.3",
      "rxjs": "5.0.1",
      "zone.js": "^0.7.4"
    }
  },
  "optionalDependencies": {
    "lodash": "^4.13.1",
    "typescript": "^2.0.6",
    "typings": "^1.3.1",
    "tslint": "^3.13.0",
    "jspm": "^0.16.39"
  }
}
